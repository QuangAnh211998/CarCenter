<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carcenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JavaClass.MailboxActivity"></activity>
        <activity android:name=".JavaClass.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JavaClass.ChangeInformationActivity" />
        <activity android:name=".JavaClass.ChangePasswordActivity" />
        <activity android:name=".JavaClass.Portpurchase" />
        <activity android:name=".JavaClass.RegisterActivity" />
        <activity android:name=".JavaClass.MainActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>